  "version": "0.0.1",
  "description": "Dashboard for mzbench",
  "main": "js/app.js",
  "dependencies": {
    "babel-core": "~5.6.0",
    "uglify-js": "~2.4.0",
    "flux": "^2.0.0",
    "react": "^0.13.0",
    "director": "^1.2.0",
    "moment": "^2.10.0",
    "moment-duration-format": "^1.3.0",
    "moment-timezone": "^0.4.0",
    "highlight.js": "^8.6.0",
    "browserify": "~10.2.0",
